{
    "name": "haltsir/semantic-date",
    "description": "Dates for humans",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Strahil Minev",
            "email": "strahil@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Haltsir\\SemanticDate\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haltsir\\SemanticDate\\SemanticDateServiceProvider"
            ]
        }
    }
}
